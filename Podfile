# platform :ios, '9.0'

target 'HackerNews' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HackerNews
  pod 'Workflow'
  pod 'WorkflowUI'
  pod 'BlueprintLists', git: 'https://github.com/kyleve/Listable.git'
  pod 'Listable', git: 'https://github.com/kyleve/Listable.git'
  pod 'BlueprintUI', git: 'https://github.com/square/Blueprint.git' 
  pod 'BlueprintUICommonControls',git: 'https://github.com/square/Blueprint.git'
end

target 'HackerNewsTests' do 
  use_frameworks!
  pod 'BlueprintUI', git: 'https://github.com/square/Blueprint.git' 
  pod 'WorkflowTesting'
end
